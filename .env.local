# Gmail Configuration for OTP
# Replace these with your actual Gmail credentials
GMAIL_USER=your.email@gmail.com
GMAIL_APP_PASSWORD=your_16_character_app_password

# Database URL (from your existing .env)
DATABASE_URL="postgresql://postgres:password@localhost:5432/habit_tracker"

# NextAuth.js
   NEXTAUTH_URL="http://localhost:3000"
   NEXTAUTH_SECRET="your-secret-key-here"

# Google OAuth (from your existing .env)
 GOOGLE_CLIENT_ID="your-google-client-id"
   GOOGLE_CLIENT_SECRET="your-google-client-secret"

# Email Configuration (from your existing .env)
 EMAIL_SERVER_HOST="smtp.gmail.com"
   EMAIL_SERVER_PORT=587
   EMAIL_SERVER_USER="your-email@gmail.com"
   EMAIL_SERVER_PASSWORD="your-app-password"
   EMAIL_FROM="noreply@habittracker.com"


